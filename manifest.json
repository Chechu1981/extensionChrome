{
  "manifest_version" : 3,
  "name": "ChechuParts",
  "version" : "1.0.0",
  "description": "Envía correos electrónicos desde Power",
  "icons":{
    "128":"./images/icon.png"
  },
  "action": {
    "icons":{
      "128": "./images/icon.png"
    }
  },
  "background":{ "service_worker": "./js/index.js" },
  "permissions": [
    "tabs",
    "notifications",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "https://power-supply.kbrw.fr/*"
  ]
}